{
  "name": "rss-feed",
  "type": "module",
  "version": "0.1.1",
  "private": true,
  "description": "RSS Feed",
  "author": "kaho",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "esno gen_data.ts && vite build",
    "build:web": "vite build",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@extractus/feed-extractor": "^7.0.4",
    "@vueuse/core": "^10.2.1",
    "date-fns": "^2.30.0",
    "esno": "^0.16.3",
    "fuse.js": "^6.6.2",
    "got": "^13.0.0",
    "p-limit": "^4.0.0",
    "pangu": "^4.0.7",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.18",
    "@types/node": "^18.16.19",
    "@types/pangu": "^4.0.0",
    "@unocss/eslint-config": "^0.53.4",
    "@unocss/reset": "^0.53.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.0",
    "eslint": "8.44.0",
    "eslint-config-kaho": "^1.1.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "naive-ui": "^2.34.4",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.11.2",
    "typescript": "5.1.6",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.4",
    "vue-tsc": "^1.8.3"
  },
  "pnpm": {
    "patchedDependencies": {}
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,cjs,mjs,ts,cts,mts}": [
      "eslint --fix"
    ],
    "package.json": [
      "eslint --fix"
    ]
  }
}
