{
  "name": "rss-feed",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "description": "RSS Feed",
  "author": "kaho",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "esno gen_data.ts && vite build",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@extractus/feed-extractor": "^6.2.2",
    "@vueuse/core": "^10.1.2",
    "date-fns": "^2.30.0",
    "esno": "^0.16.3",
    "fuse.js": "^6.6.2",
    "ky": "^0.33.3",
    "p-limit": "^4.0.0",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@types/node": "^18.16.11",
    "@unocss/eslint-config": "^0.51.13",
    "@unocss/reset": "^0.51.13",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.9.8",
    "@vue/test-utils": "^2.3.2",
    "eslint": "8.39.0",
    "eslint-config-kaho": "^0.9.9",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.5.1",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^2.1.7",
    "vite": "^4.3.7",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "kaho/vue",
      "@unocss"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "extraFileExtensions": [
        ".vue"
      ]
    }
  }
}
