{
  "name": "rss-feed",
  "type": "module",
  "version": "0.1.1",
  "private": true,
  "description": "RSS Feed",
  "author": "kaho",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "esno gen_data.ts && vite build",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@extractus/feed-extractor": "^6.2.2",
    "@vueuse/core": "^10.1.2",
    "date-fns": "^2.30.0",
    "esno": "^0.16.3",
    "fuse.js": "^6.6.2",
    "ky": "^0.33.3",
    "p-limit": "^4.0.0",
    "pangu": "^4.0.7",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.17",
    "@types/node": "^18.16.16",
    "@types/pangu": "^4.0.0",
    "@unocss/eslint-config": "^0.52.5",
    "@unocss/reset": "^0.52.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.3.2",
    "eslint": "8.41.0",
    "eslint-config-kaho": "^0.9.9",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "naive-ui": "^2.34.4",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.1",
    "typescript": "^5.0.4",
    "unocss": "^0.52.5",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vitest": "^0.31.2",
    "vue-tsc": "^1.6.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,cjs,mjs,ts,cts,mts}": [
      "eslint --fix"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "@extractus/feed-extractor@6.2.2": "patches/@extractus__feed-extractor@6.2.2.patch"
    }
  }
}
